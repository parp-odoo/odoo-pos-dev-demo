<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_urban_piper.DebugWidget" t-inherit="point_of_sale.DebugWidget" t-inherit-mode="extension">
        <xpath expr="//div[hasclass(debug-others)]" position="after">
            <div t-if="showUrbanPiperTestOrderBtn" data-tooltip="Place Quick UrbanPiper Test Order" data-tooltip-delay="0">
                <h5 class="category">UrbanPiper</h5>
                  <ul class="mb-3">
                    <li class="button btn btn-link" t-on-click="() => placeUrbanPiperQuickTestOrder">
                        Quick Order
                    </li>
                </ul>
            </div>
        </xpath>
    </t>
</templates>
